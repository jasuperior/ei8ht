{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "ts-output",
        "jsxFactory": "createUnitTree",
        "jsx": "react"
        // "jsxImportSource": "./"
        // "jsxImportSource": "./api/index"
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"]
}
